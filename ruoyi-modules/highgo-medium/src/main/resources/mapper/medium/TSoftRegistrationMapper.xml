<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.highgo.medium.mapper.TSoftRegistrationMapper">
    
    <resultMap type="TSoftRegistration" id="TSoftRegistrationResult">
        <result property="id"    column="id"    />
        <result property="name"    column="name"    />
        <result property="registrationNum"    column="registration_num"    />
        <result property="createId"    column="create_id"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateId"    column="update_id"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
    </resultMap>

    <sql id="selectTSoftRegistrationVo">
        select id, name, registration_num, create_id, create_by, create_time, update_id, update_by, update_time from t_soft_registration
    </sql>

    <select id="selectTSoftRegistrationList" parameterType="TSoftRegistration" resultMap="TSoftRegistrationResult">
        <include refid="selectTSoftRegistrationVo"/>
        <where>  
            <if test="name != null  and name != ''"> and name like concat('%', #{name}, '%')</if>
            <if test="registrationNum != null  and registrationNum != ''"> and registration_num = #{registrationNum}</if>
            <if test="createId != null "> and create_id = #{createId}</if>
            <if test="updateId != null "> and update_id = #{updateId}</if>
        </where>
    </select>
    
    <select id="selectTSoftRegistrationById" parameterType="Long" resultMap="TSoftRegistrationResult">
        <include refid="selectTSoftRegistrationVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertTSoftRegistration" parameterType="TSoftRegistration" useGeneratedKeys="true" keyProperty="id">
        insert into t_soft_registration
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="name != null and name != ''">name,</if>
            <if test="registrationNum != null and registrationNum != ''">registration_num,</if>
            <if test="createId != null">create_id,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateId != null">update_id,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="name != null and name != ''">#{name},</if>
            <if test="registrationNum != null and registrationNum != ''">#{registrationNum},</if>
            <if test="createId != null">#{createId},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateId != null">#{updateId},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
         </trim>
    </insert>

    <update id="updateTSoftRegistration" parameterType="TSoftRegistration">
        update t_soft_registration
        <trim prefix="SET" suffixOverrides=",">
            <if test="name != null and name != ''">name = #{name},</if>
            <if test="registrationNum != null and registrationNum != ''">registration_num = #{registrationNum},</if>
            <if test="createId != null">create_id = #{createId},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateId != null">update_id = #{updateId},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteTSoftRegistrationById" parameterType="Long">
        delete from t_soft_registration where id = #{id}
    </delete>

    <delete id="deleteTSoftRegistrationByIds" parameterType="String">
        delete from t_soft_registration where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>